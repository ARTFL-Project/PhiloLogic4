        "bootstrap": "^5.0.1",
        "chart.js": "^2.9.3",
        "core-js": "^3.13.1",
        "velocity-animate": "^1.5.2",
        "vue": "^3.2.0",
        "vue-router": "^4.0.0",
        "vue-scrollto": "^2.17.1",
        "vuex": "^4.0.0",
        "vuex-map-fields": "^1.4.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.13",
        "@vue/cli-plugin-eslint": "~4.5.13",
        "@vue/cli-service": "^4.5.14",
        "@vue/compiler-sfc": "^3.1.0",
